    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background2">

    <TableLayout
        android:id="@+id/tableLayout1"
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:shrinkColumns="*"
        android:stretchColumns="*">

        <TableRow
            android:id="@+id/Row1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal" >

            <TextView
                android:id="@+id/textView1_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_span="6"
                android:gravity="center"
                android:text="~TITLE~"
                android:textSize="18dp"
                android:textStyle="bold" >
            </TextView>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Next Week"
                android:id="@+id/nextWeekButton"
                android:layout_column="1"
                android:layout_span="2"
                android:onClick="updateWeek"
                android:background="#000000" />
        </TableRow>

        <TableRow
            android:id="@+id/Row2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/TextView2_1"
                android:gravity="center"
                android:text="Team Names"
                android:textStyle="bold"
                android:typeface="serif">
            </TextView>

            <TextView
                android:id="@+id/TextView2_2"
                android:gravity="center"
                android:text="Power Rankings"
                android:textStyle="bold"
                android:typeface="serif" >
            </TextView>

            <TextView
                android:id="@+id/TextView2_3"
                android:gravity="center"
                android:text="Bayes Predictions"
                android:textStyle="bold"
                android:typeface="serif" >
            </TextView>

            <TextView
                android:id="@+id/TextView2_4"
                android:gravity="center"
                android:text="3.14 Rankings"
                android:textStyle="bold"
                android:typeface="serif" >
            </TextView>
        </TableRow>



        </TableLayout>

